---
import HeaderReact from "./HeaderReact";
import { fetchNavEntries } from "./Navigation/nav-tree-server";

const { projectName, slug } = Astro.props;
const entries = fetchNavEntries(slug);
---

<HeaderReact
  title={projectName}
  homeUrl={import.meta.env.BASE_URL}
  navEntries={entries}
  client:load
/>
